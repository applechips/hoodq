<p id="notice"><%= notice %></p>

<h1>Map Data</h1>


<%= @data_hash[0] %>

<table>
  <thead>
    <tr>
      <th>Address</th>
      <th>Nearestcoffeeshop</th>
      <th>Workdistance</th>
      <th>Nearbydogpark</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @map_data.each do |map_datum| %>
      <tr>
        <td><%= map_datum.address %></td>
        <td><%= map_datum.nearestCoffeeShop %></td>
        <td><%= map_datum.workDistance %></td>
        <td><%= map_datum.nearbyDogPark %></td>
        <td><%= link_to 'Show', map_datum %></td>
        <td><%= link_to 'Edit', edit_map_datum_path(map_datum) %></td>
        <td><%= link_to 'Destroy', map_datum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

